{
  "name": "sagw",
  "version": "0.0.1",
  "description": "Codebase for SAGW websites.",
  "author": "Vorhall KlG <info@vorhall.com> (https://vorhall.com)",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "concurrently \"npm run start:db\" \"npm run dev:nodb\"",
    "dev:nodb": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint:js": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:scss": "stylelint '**/*.scss'",
    "lint": "npm run lint:js && npm run lint:scss",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "storybook": "storybook dev -p 6006",
    "storybook:ci": "storybook dev -p 6006 --ci",
    "build-storybook": "storybook build",
    "test:native": "playwright test",
    "test:native:watch": "playwright test --ui-port=8080 --ui-host=0.0.0.0",
    "docker:test:build": "sudo docker compose -f docker-compose.testing.yml build",
    "test": "docker compose -f docker-compose.testing.yml run --rm visual-test npm run test:native",
    "test:watch": "docker compose -f docker-compose.testing.yml run --rm -p 8080:8080 visual-test npm run test:native:watch",
    "start:db": "docker compose -f docker-compose.db.yml run --rm -p 27017:27017 mongodb --quiet --syslog",
    "generate:component": "tsx convenience/generate-component/index",
    "restore:blob": "tsx ./src/backup-restore/restores/blob-restore",
    "restore:db": "tsx ./src/backup-restore/restores/db-restore"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/payload-cloud": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "next": "15.2.2",
    "payload": "latest",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@aws-sdk/lib-storage": "^3.782.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "^1.51.1",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/blocks": "^8.6.9",
    "@storybook/nextjs": "^8.6.12",
    "@storybook/react": "^8.6.9",
    "@storybook/test": "^8.6.9",
    "@storybook/types": "^8.6.12",
    "@stylistic/stylelint-config": "^2.0.0",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "@types/node": "^22.14.0",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vercel/blob": "^0.27.3",
    "boxen": "^8.0.1",
    "bson": "^6.10.3",
    "chalk": "^5.4.1",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.24.0",
    "eslint-config-next": "15.2.4",
    "eslint-plugin-storybook": "^0.12.0",
    "husky": "^9.1.7",
    "inquirer": "^12.5.2",
    "lint-staged": "^15.5.0",
    "mongodb": "^6.15.0",
    "resend": "^4.2.0",
    "sass": "^1.86.3",
    "storybook": "^8.6.12",
    "stylelint": "^16.18.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "tsx": "^4.19.3",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "22.14.0"
  }
}